{% extends '@AltezzaTravel/layout_tour.html.twig' %}

{%- block title -%}
    Oops! -

    {{ parent() }}
{%- endblock -%}

{% block template %}
    <div class="content-page">
        <div class="page error-page js-full-height" style="margin-top: 0; min-height: 357.5px;">
            <div class="error">
                <br><br><br>
                <div class="ep__code">Oops!</div>
                <br>
                <div class="ep__desc">{{ message }}</div>
                <div class="ep__link">
                    <br><br>
                    <a class="btn admin-btn" href="{{ url('tour_dashboard') }}">Go to dashboard</a>
                </div>
            </div>
        </div>
    </div>
{% endblock %}
